<?xml version="1.0" ?>
<Animations>
    <AnimationDefinition name="FlyIn" duration="0.5" replayMode="once">
        <Affector property="Position" interpolator="UVector2">
            <KeyFrame position="0" value="{{0,0},{0,-480}}" />
            <KeyFrame position="0.5" value="{{0,0},{0,0}}" progression="quadratic decelerating" />
        </Affector>
    </AnimationDefinition>
    
    <AnimationDefinition name="FlyOut" duration="0.5" replayMode="once">
        <Affector property="Position" interpolator="UVector2">
            <KeyFrame position="0" value="{{0,0},{0,0}}" />
            <KeyFrame position="0.5" value="{{0,0},{0,480}}" progression="quadratic accelerating" />
        </Affector>
    </AnimationDefinition>
    
    <AnimationDefinition name="FakeProgress" duration="1.0" replayMode="once">
        <Affector property="CurrentProgress" interpolator="float">
            <KeyFrame position="0" value="0.0" />
            <KeyFrame position="1.0" value="1.0" />
        </Affector>
    </AnimationDefinition>
    
    <AnimationDefinition name="PulseIn" duration="0.5" replayMode="once">
        <Affector property="Alpha" interpolator="float">
            <KeyFrame position="0" value="0" />
            <KeyFrame position="0.5" value="1" progression="quadratic accelerating" />
        </Affector>
    </AnimationDefinition>
    
    <AnimationDefinition name="PulseOut" duration="0.1" replayMode="once">
        <Affector property="Alpha" interpolator="float">
            <KeyFrame position="0" value="1" />
            <KeyFrame position="0.1" value="0" />
        </Affector>
    </AnimationDefinition>
    
    <AnimationDefinition name="None" duration="0" replayMode="once">
    
    </AnimationDefinition>
</Animations>

