<GUILayout version="4"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../xml_schemas/GUILayout.xsd">
    <Window name="Pause" type="DefaultWindow">
        <Property name="Area" value="{{0,0},{0,0},{0,640},{0,480}}" />
        <Window name="Panel" type="GlossySerpent/SimplePanel">
            <Property name="Area" value="{{0,50},{0,50},{1,-50},{1,-50}}" />
            <Window name="Titlebar" type="GlossySerpent/Titlebar">
                <Property name="Area" value="{{0,0},{0,0},{1,0},{0,30}}" />
                <Property name="Text" value="ForestRunner Paused" />
                <Property name="AlwaysOnTop" value="True" />
            </Window>
            <Window name="MultiLineEditbox" type="GlossySerpent/MultiLineEditbox">
                <Property name="Area" value="{{0,20},{0,50},{0.5,0},{1,-20}}" />
                <Property name="Text">Instructions:

dodge:
   [left] and [right] arrow keys

pause:
    [space bar]
</Property>
                <Property name="ReadOnly" value="True" />
            </Window>
            <Window name="StaticText" type="GlossySerpent/StaticText">
                <Property name="Area" value="{{0,279},{0,50},{1,-11},{0,80}}" />
                <Property name="Text" value="Speed:" />
                <Property name="FrameEnabled" value="False" />
                <Property name="BackgroundEnabled" value="False" />
            </Window>
            <Window name="sb_speed" type="GlossySerpent/HorizontalScrollbar">
                <Property name="Area" value="{{0,280},{0,80},{1,-10},{0,100}}" />
                <Property name="PageSize" value="1" />
                <Property name="DocumentSize" value="10" />
            </Window>
            <Window name="StaticText1" type="GlossySerpent/StaticText">
                <Property name="Area" value="{{0,279},{0,120},{1,-11},{0,150}}" />
                <Property name="Text" value="Density:   " />
                <Property name="FrameEnabled" value="False" />
                <Property name="BackgroundEnabled" value="False" />
            </Window>
            <Window name="sb_density" type="GlossySerpent/HorizontalScrollbar">
                <Property name="Area" value="{{0,280},{0,150},{1,-10},{0,170}}" />
                <Property name="PageSize" value="1" />
                <Property name="DocumentSize" value="10" />
            </Window>
            <Window name="StaticText2" type="GlossySerpent/StaticText">
                <Property name="Area" value="{{0,279},{0,190},{1,-11},{0,220}}" />
                <Property name="Text" value="Radius:   " />
                <Property name="FrameEnabled" value="False" />
                <Property name="BackgroundEnabled" value="False" />
            </Window>
            <Window name="sb_radius" type="GlossySerpent/HorizontalScrollbar">
                <Property name="Area" value="{{0,280},{0,220},{1,-10},{0,240}}" />
                <Property name="PageSize" value="1" />
                <Property name="DocumentSize" value="10" />
            </Window>
            <Window name="btn_new" type="GlossySerpent/Button">
                <Property name="Area" value="{{0,300},{0,270},{1,-30},{0,300}}" />
                <Property name="Text" value="New Game" />
            </Window>
            <Window name="btn_advanced" type="GlossySerpent/Button">
                <Property name="Area" value="{{0,300},{0,300},{1,-30},{0,330}}" />
                <Property name="Text" value="Advanced Settings" />
            </Window>
            <Window name="btn_resume" type="GlossySerpent/Button">
                <Property name="Area" value="{{0,300},{0,330},{1,-30},{0,360}}" />
                <Property name="Text" value="Cancel/Resume" />
            </Window>
        </Window>
    </Window>
</GUILayout>